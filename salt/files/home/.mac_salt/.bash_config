if [ -f .git-completion.bash ]; then
	source .git-completion.bash
fi

if [ -f .git-prompt.sh ]; then
	source .git-prompt.sh
fi

if [ -f .profile ]; then
	source .profile
fi

if [ -f .aliases ]; then
	source .aliases
fi

if [ -f .functions ]; then
	source .functions
fi

if [[ -d "$HOME/.rbenv" ]] ; then
	export PATH="$HOME/.rbenv/bin:$PATH"
	eval "$(rbenv init -)";
	export RBENV_ROOT=$(rbenv root)
fi